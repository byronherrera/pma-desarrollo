ALTER TABLE `amc_denuncias`
ADD COLUMN `recepcion_inspeccion`  int(1) NULL DEFAULT 0 AFTER `guia`;

ALTER TABLE `amc_denuncias`
ADD COLUMN `procesado_inspeccion`  int(1) NULL DEFAULT 0 AFTER `recepcion_inspeccion`;

ALTER TABLE `prueba`.`amc_denuncias`
ADD COLUMN `id_planificacion` int(1) NULL AFTER `procesado_inspeccion`;
ADD COLUMN `id_motivo_acta` int(1) NULL AFTER `id_planificacion`;
ADD COLUMN `id_acta` varchar(255) NULL AFTER `id_motivo_acta`;
ADD COLUMN `prioridad` int(1) NULL AFTER `id_acta`;
ADD COLUMN `funcionario_reasignacion` int(1) NULL AFTER `prioridad`;
ADD COLUMN `fecha_asignacion` varchar(255) NULL AFTER `funcionario_reasignacion`;




ALTER TABLE `prueba`.`amc_inspeccion_control_programado`
ADD COLUMN `id` int(11) NOT NULL AFTER `nombre_denunciado`,
ADD COLUMN `id_denuncia` int(11) NULL AFTER `id`,
ADD COLUMN `id_inspeccion` int(11) NULL AFTER `id_denuncia`,
ADD PRIMARY KEY (`id`, `id`) USING BTREE;
ADD COLUMN `fecha_registro` datetime(0) NULL AFTER `id_inspeccion`,
ADD COLUMN `codigo_tramite` varchar(255) NULL AFTER `fecha_registro`,
ADD COLUMN `tecnico` varchar(255) NULL AFTER `codigo_tramite`,
ADD COLUMN `fecha_asignacion_inspector` timestamp NULL AFTER `tecnico`;
ADD COLUMN `asunto` varchar(255) NULL AFTER `fecha_asignacion_inspector`,
ADD COLUMN `sector` varchar(255) NULL AFTER `asunto`,
ADD COLUMN `parroquia` varchar(255) NULL AFTER `sector`,
ADD COLUMN `zona` varchar(255) NULL AFTER `parroquia`,
ADD COLUMN `calle` varchar(255) NULL AFTER `zona`,
ADD COLUMN `predio` varchar(255) NULL AFTER `calle`,
ADD COLUMN `clave_catastral` varchar(255) NULL AFTER `predio`,
ADD COLUMN `inventariado` varchar(255) NULL AFTER `clave_catastral`,
ADD COLUMN `nombre_propietario` varchar(255) NULL AFTER `inventariado`,
ADD COLUMN `cedula_propietario` varchar(255) NULL AFTER `nombre_propietario`,
ADD COLUMN `proyecto` varchar(255) NULL AFTER `cedula_propietario`,
ADD COLUMN `etapas` varchar(255) NULL AFTER `proyecto`,
ADD COLUMN `tramite` varchar(255) NULL AFTER `etapas`,
ADD COLUMN `aprobacion_registro` varchar(255) NULL AFTER `tramite`,
ADD COLUMN `telefono` varchar(255) NULL AFTER `aprobacion_registro`,
ADD COLUMN `registro_actas_licencias` varchar(255) NULL AFTER `telefono`,
ADD COLUMN `gdoc` varchar(255) NULL AFTER `registro_actas_licencias`,
ADD COLUMN `responsable_tecnico` varchar(255) NULL AFTER `gdoc`,
ADD COLUMN `licencia_profesional` varchar(255) NULL AFTER `responsable_tecnico`,
ADD COLUMN `licencia_municipal` varchar(255) NULL AFTER `licencia_profesional`;




ALTER TABLE `prueba`.`amc_inspeccion_ccf`
ADD COLUMN `id` int(11) NOT NULL AFTER `nombre_denunciado`,
ADD COLUMN `id_denuncia` int(11) NULL AFTER `id`,
ADD COLUMN `id_inspeccion` int(11) NULL AFTER `id_denuncia`,
ADD PRIMARY KEY (`id`, `id`) USING BTREE;
ADD COLUMN `fecha_registro` datetime(0) NULL AFTER `id_inspeccion`,
ADD COLUMN `num_registro` varchar(255) NULL AFTER `fecha_registro`,
ADD COLUMN `tecnico` varchar(255) NULL AFTER `num_registro`,
ADD COLUMN `fecha_asignacion_inspector` datetime NULL AFTER `tecnico`;
ADD COLUMN `fecha_inicio` datetime NULL AFTER `fecha_asignacion_inspector`,
ADD COLUMN `fecha_finalización` datetime NULL AFTER `fecha_inicio`,
ADD COLUMN `asunto` varchar(255) NULL AFTER `fecha_finalización`,
ADD COLUMN `tipo` varchar(255) NULL AFTER `asunto`,
ADD COLUMN `zona` varchar(255) NULL AFTER `tipo`,
ADD COLUMN `predio` varchar(255) NULL AFTER `zona`,
ADD COLUMN `clave_catastral` varchar(255) NULL AFTER `predio`,
ADD COLUMN `proyecto` varchar(255) NULL AFTER `clave_catastral`,
ADD COLUMN `etapa` varchar(255) NULL AFTER `proyecto`,
ADD COLUMN `fecha_inspeccion` datetime NULL AFTER `etapa`,
